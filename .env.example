# LiteLLM Configuration (required for LLM subtitle generation)
LITELLM_MASTER_KEY=your_litellm_master_key_here
LITELLM_API_BASE_URL=http://localhost:4000
LITELLM_MODEL=qwen3-coder

# Service-specific API Keys (choose one based on your LITELLM_MODEL)
GROK_API_KEY=your_grok_api_key_here        # For grok-code-fast-1 model
DASHSCOPE_API_KEY=your_dashscope_api_key_here # For qwen3-coder model
Z_API_KEY=your_zhipu_api_key_here           # For glm-4.5 model

# Volcengine TTS Configuration (required for audio generation)
VOLCENGINE_APP_ID=your_volcengine_app_id_here
VOLCENGINE_ACCESS_TOKEN=your_volcengine_access_token_here

# Optional: Custom fonts path (if needed)
CUSTOM_FONTS_PATH=/path/to/fonts